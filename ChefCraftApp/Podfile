use_frameworks!
inhibit_all_warnings!

target 'ChefCraftApp' do
  platform :ios, '13.0'
  # Comment the next line if you don't want to use dynamic frameworks

  # Pods for ChefCraftApp
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxDataSources'
  pod 'RxRealm'
  pod 'Swinject'
  pod 'RealmSwift'
  pod 'SwiftEntryKit'
  
